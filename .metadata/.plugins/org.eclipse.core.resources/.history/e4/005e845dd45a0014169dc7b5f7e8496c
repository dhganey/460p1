package creationalPatterns;

public class GBHTMLAllDayRestaurant extends Restaurant implements IReader, IMenuGenerator, IMenuFormatter
{
	public GBHTMLAllDayRestaurant()
	{
		//TODO
	}
	
	public FileReader getReader()
	{
		reader = new XMLReader();
		return reader;
	}

	public MenuGenerator getMenuGenerator()
	{
		generator = new MenuGenerator(Country.GB, RestaurantType.DINER);
		return generator;
	}

	public MenuFormatter getMenuFormatter()
	{
		formatter = new HTMLMenuFormatter();
		return formatter;
	}
}
